<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="shifaz.sikkander.com.hourgrid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HourGridActivity"/>
        <activity android:name=".HourGridSundayActivity"/>
        <activity android:name=".HourGridMondayActivity"/>
        <activity android:name=".HourGridTuesdayActivity"/>
        <activity android:name=".HourGridWednesdayActivity"/>
        <activity android:name=".HourGridThursdayActivity"/>
        <activity android:name=".HourGridFridayActivity"/>
        <activity android:name=".HourGridSaturdayActivity"/>
        <activity android:name=".CalendarActivity"/>
        <activity android:name=".FrameLayoutActivity"/>
    </application>

</manifest>